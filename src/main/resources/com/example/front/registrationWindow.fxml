<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mainPane" prefHeight="482.0" prefWidth="457.0" style="-fx-background-color: White;"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.front.RegistrationController">
    <AnchorPane fx:id="dataPane" layoutX="10.0" layoutY="10.0" style="-fx-background-color: #b3b3b3;"
                AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
                AnchorPane.topAnchor="5.0">
        <Label layoutX="104.0" layoutY="17.0" prefHeight="54.0" prefWidth="343.0" text="зарегестрироваться"
               textFill="WHITE" AnchorPane.topAnchor="17.0">
            <font>
                <Font name="Segoe UI Semibold" size="30.0"/>
            </font>
        </Label>
        <Label layoutX="50.0" layoutY="84.0" text="имя" textFill="WHITE" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="nameText" layoutX="50.0" layoutY="114.0" prefHeight="30.0" prefWidth="190.0"
                   style="-fx-background-color: white; -fx-background-radius: 0;" AnchorPane.leftAnchor="50.0"
                   AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </TextField>
        <Label layoutX="50.0" layoutY="144.0" text="логин" textFill="WHITE" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="loginText" layoutX="49.0" layoutY="174.0" prefHeight="30.0" prefWidth="190.0"
                   style="-fx-background-color: white; -fx-background-radius: 0;" AnchorPane.leftAnchor="49.0"
                   AnchorPane.rightAnchor="51.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </TextField>
        <Label layoutX="50.0" layoutY="204.0" text="токен" textFill="WHITE" AnchorPane.leftAnchor="50.0"
               AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="tokenText" layoutX="49.0" layoutY="234.0" prefHeight="30.0" prefWidth="190.0"
                   style="-fx-background-color: white; -fx-background-radius: 0;" AnchorPane.leftAnchor="49.0"
                   AnchorPane.rightAnchor="51.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </TextField>
        <Label fx:id="addressLabel" layoutX="50.0" layoutY="264.0" text="хост" textFill="WHITE"
               AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="20.0"/>
            </font>
        </Label>
        <TextField fx:id="addressText" layoutX="50.0" layoutY="294.0" prefHeight="30.0" prefWidth="190.0"
                   style="-fx-background-color: white; -fx-background-radius: 0;" AnchorPane.leftAnchor="50.0"
                   AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </TextField>
        <Button layoutX="49.0" layoutY="360.0" mnemonicParsing="false" onAction="#onRegistrationButtonClick"
                prefHeight="25.0" prefWidth="203.0" style="-fx-background-color: #00DDC0; -fx-background-radius: 0;"
                text="зарегистрироваться" textFill="WHITE" AnchorPane.leftAnchor="49.0" AnchorPane.rightAnchor="51.0">
            <font>
                <Font name="Segoe UI Semibold" size="16.0"/>
            </font>
        </Button>
        <CheckBox fx:id="memoryCheck" contentDisplay="RIGHT" layoutX="49.0" layoutY="324.0" mnemonicParsing="false"
                  prefHeight="36.0" prefWidth="217.0" text="запомнить меня" textAlignment="CENTER" textFill="WHITE"
                  AnchorPane.leftAnchor="49.0" AnchorPane.rightAnchor="51.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </CheckBox>
        <Label fx:id="errorLabel" alignment="TOP_CENTER" layoutX="50.0" layoutY="429.0" textFill="RED"
               AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </Label>
        <Button fx:id="loginButton" layoutX="50.0" layoutY="399.0" mnemonicParsing="false"
                onAction="#onLoginButtonClick" prefHeight="30.0" prefWidth="347.0"
                style="-fx-background-color: #b3b3b3;" text="войти" textFill="WHITE">
            <font>
                <Font name="Segoe UI Semibold" size="14.0"/>
            </font>
        </Button>
        <ImageView fitHeight="46.0" fitWidth="42.0" layoutX="50.0" layoutY="24.0" pickOnBounds="true"
                   preserveRatio="true" AnchorPane.topAnchor="24.0">
            <Image url="@images/profile.png"/>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="397.0" layoutY="114.0" onMouseClicked="#nameInfoClick"
                   pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="397.0">
            <Image url="@images/light-bulb.png"/>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="396.0" layoutY="174.0" onMouseClicked="#loginInfoClick"
                   pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="397.0">
            <Image url="@images/light-bulb.png"/>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="397.0" layoutY="234.0" onMouseClicked="#tokenInfoClick"
                   pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="397.0">
            <Image url="@images/light-bulb.png"/>
        </ImageView>
        <ImageView fitHeight="30.0" fitWidth="30.0" layoutX="397.0" layoutY="294.0" onMouseClicked="#addressInfoClick"
                   pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="397.0">
            <Image url="@images/light-bulb.png"/>
        </ImageView>

    </AnchorPane>
</AnchorPane>
